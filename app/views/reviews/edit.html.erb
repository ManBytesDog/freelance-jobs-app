<h1>Review Edit Page</h1>
<%if flash[:errors]%>
<% flash[:errors].each do |error| %>
    <p style="color: Red;"><%= error%> <br></p>
<%end%>
<%end%>

<%= form_for @review do | f | %>
         
            <%= f.label :review %>
            <br/>
            <%= f.text_area :review %>
            <br/>
            <%= f.label :rating %>
            <br/>
            <%= f.number_field :rating, step: :any %>

            <br/>
            <%= f.submit %>
<%end%>