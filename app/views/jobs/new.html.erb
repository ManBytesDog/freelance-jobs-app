<h1> Create A New Job Post </h1>

<%= form_for @job do |f|%>
    <%= f.label :job_title %>
    <%= f.text_field :job_title %> <br>

    <%= f.label :job_type %>
    <%= f.text_field :job_type %> <br>

    <%= f.label :description %>
    <%= f.text_area :description %> <br>

    <%= f.label :remote %>
    <%= f.text_field :remote_job %> <br>

    <%= f.label :compensation %>
    <%= f.text_field :compensation %> <br>

    <%= f.submit %>
<%end%>